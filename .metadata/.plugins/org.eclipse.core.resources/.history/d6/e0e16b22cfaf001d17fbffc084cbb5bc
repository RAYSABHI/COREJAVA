package in.co.If_Else;

public class IfElseLadder {
	public static void main(String[] args) {
		
		int x = 10;
		int y = 20;
		int z = 30;
		
		if (x == 10) {
			System.out.println("true vale of x");
		}
		else if (x == y) {
			System.out.println("value of x and y same");
		}
		else if (x == z) {
			System.out.println("vale of x and z same");
		}
		else {
			System.out.println("no value find");
		}
		
	}

}
