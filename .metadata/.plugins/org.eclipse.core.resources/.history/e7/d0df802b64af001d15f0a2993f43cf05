package in.co.inheritance;

public class ArrayTest {
	
	public static void main(String[] args) {
		
		double sum = 0.0;
		Shape s1[] = new Shape[3];
		
		s1[0] = new Rectangle();
		Rectangle r = (Rectangle) s1[0];
		r.setLength(10);
		r.setWidth(10);
		r.area();
	
		System.out.println("area of rectangle : " +r.area());
	}

}
