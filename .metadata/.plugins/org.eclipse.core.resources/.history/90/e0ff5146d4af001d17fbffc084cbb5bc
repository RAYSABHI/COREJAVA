package in.co.If_Else;

public class IfElse3 {
	public static void main(String[] args) {
		
		int size = args.length;
		if (size == 0) {
			System.out.println("if condition is true and run");
		}
		else {
			for(int i = 0; i<size; i++) {
				System.out.println(i + "Hello "+ args[i]);
			}
		}
	}

}
