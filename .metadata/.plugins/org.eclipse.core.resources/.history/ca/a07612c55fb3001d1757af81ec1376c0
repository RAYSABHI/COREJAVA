		r.setLength(50);
		r.setWidth(50);
		System.out.println("area of rectangle : " +r.area());
		
		//..........................................
		
		Q1_Circle c = new Q1_Circle();
		
		c.setRadius(20);
		
		System.out.println("area of circle : " +c.area());
		
		//...........................................
		
		Q1_Triangle t = new Q1_Triangle();
		t.setBase(20);
		t.setHight(30);
	}

}
