package in.co.If_Else;

public class Ladder_IfElse2 {
	public static void main(String[] args) {
		
		String city = "Delhi";
		
		if (city == "Mumbai") {
			System.out.println("city is mumbai");
		}
		
		else if (city == "Pune") {
			System.out.println("city is pune");
		}
		else if (city == "Banglore") {
			System.out.println("city is banglore");
		}
		else {
			System.out.println("City Delhi is capital");
		}
		
	}

}
